	// =================================================
	// SEMANTIC TOKEN COLORS
	// =================================================
	"semanticTokenColors": {
		// --- Types ---
		"class": "#ffffff", // Class names like "User", "Database", "Component"
		"interface": "#ffffff", // Interface definitions like "IUserService", "ApiResponse"
		"enum": "#ffffff", // Enumeration types like "StatusCode", "Color"
		"struct": "#ffffff", // Struct definitions in languages like C#, Rust
		"type": "#ffffff", // Type aliases and custom types like "UserId", "Callback"
		"typeParameter": "#ffffff", // Generic type parameters like "T", "K", "V"
		// --- Variables ---
		"variable": "#ffffff", // Variable names like "userName", "count", "data"
		"parameter": "#ffffff", // Function parameters like "id", "options", "callback"
		"property": "#ffffff", // Object properties like "user.name", "config.timeout"
		"enumMember": "#ffffff", // Enum values like "Status.Active", "Color.Red"
		// --- Values and Literals ---
		"string": "#ffffff", // String literals like "Hello World", 'path/to/file'
		"number": "#ffffff", // Numeric values like 42, 3.14, 0xFF, 1e5, -100
		"boolean": "#ffffff", // Boolean values: true, false
		"regexp": "#ffffff", // Regular expressions like /pattern/gi, new RegExp()
		// --- Functions ---
		"function": "#ffffff", // Function names like "calculateTotal", "fetchData"
		"method": "#ffffff", // Method calls like "user.getName()", "array.push()"
		// --- Keywords ---
		"keyword": "#ffffff", // Language keywords like "if", "for", "return", "class"
		"modifier": "#ffffff", // Access modifiers like "public", "private", "static"
		"namespace": "#ffffff", // Namespace declarations like "MyApp.Utils", "System.IO"
		// --- Comments ---
		"comment": "#ffffff", // Comment text: // single line, /* block comments */
		// --- Special Modifiers ---
		"*.static": {
			"fontStyle": "italic" // Static members shown in italic: static methods, fields
		},
		"*.readonly": {
			"fontStyle": "bold" // Readonly/immutable items shown in bold: const, readonly
		},
		"*.decorator": {
			"fontStyle": "italic", // Decorators shown in italic: @Component, @Injectable
			"foreground": "#ffffff"
		},
		"*.abstract": {
			"fontStyle": "italic" // Abstract classes/methods shown in italic
		}
	},
	// =================================================
	// TOKEN COLORS (TEXTMATE)
	// =================================================
	"tokenColors": [
		// --- Base Text & Structure ---
		{
			"scope": [
				"source", // Base source code text
				"support.type.property-name.css" // CSS property names like "color", "margin"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"punctuation", // General punctuation marks
				"punctuation.terminator", // Statement terminators like semicolons
				"punctuation.definition.tag", // HTML/XML tag punctuation < >
				"punctuation.separator", // Separators like commas, periods
				"punctuation.definition.string", // String quote marks " '
				"punctuation.section.block" // Block delimiters like { } [ ]
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		// --- Classes & Types ---
		{
			"scope": [
				"entity.name.type.class" // Class definitions: class User, class Component
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"entity.name.type.interface", // Interface definitions: interface IUser
				"entity.name.type" // General type names
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"entity.name.type.struct" // Struct definitions in C#, Rust: struct Point
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"entity.name.type.enum" // Enum definitions: enum Status, enum Color
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"support.type" // Built-in types like string, number, boolean
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"variable.type.parameter", // Parameter types in function signatures
				"variable.parameter.type"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		// --- Functions & Methods ---
		{
			"scope": [
				"entity.name.function.method", // Method definitions: getName(), calculateTotal()
				"meta.function.method" // Method declarations and calls
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"entity.name.function", // Function names: function processData()
				"support.function", // Built-in functions like console.log(), Math.max()
				"meta.function-call.generic" // Generic function calls
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"variable.function" // Function variables: const handler = () => {}
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"entity.name.function.preprocessor", // Preprocessor functions in C/C++
				"meta.preprocessor" // Preprocessor directives like #define, #include
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"meta.preprocessor" // Additional preprocessor scope coverage
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"meta.decorator", // Decorators: @Component, @Injectable, @Override
				"punctuation.decorator", // Decorator symbols @
				"entity.name.function.decorator" // Decorator function names
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		// --- Variables & Properties ---
		{
			"scope": [
				"variable", // Variable names: let userName, const config
				"meta.variable", // Variable declarations and references
				"variable.other.object.property", // Object properties: user.name, obj.value
				"variable.other.readwrite.alias" // Variable aliases and references
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"variable.other.object" // Object variable references: myObject, userdata
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"variable.other.global", // Global variables: window, document, process
				"variable.language.this" // 'this' keyword in object contexts
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"variable.other.local" // Local scope variables within functions/blocks
			],
			"settings": {
				"foreground": "#ffffff"

{
	"$schema": "vscode://schemas/color-theme",
	"type": "light",
	"name": "Template",
	// =================================================
	// WORKBENCH UI COLORS
	// =================================================
	"colors": {
		// --- Editor Core ---
		"editor.background": "#ffffff", // Main editor workspace background - the primary canvas where code is written
		"editor.foreground": "#ffffff", // Default text color for all content in the editor
		"editor.lineHighlightBackground": "#ffffff", // Background color for the currently active line highlight
		"editor.lineHighlightBorder": "#ffffff", // Border color around the current line highlight
		"editor.selectionBackground": "#ffffff", // Background color for selected text in the editor
		"editor.selectionHighlightBackground": "#ffffff00", // Background for other instances of the same selected text (must be opaque)
		"editor.inactiveSelectionBackground": "#ffffff00", // Selection background when editor loses focus (must be opaque)
		"editor.wordHighlightBackground": "#ffffff00", // Background for highlighting matching words under cursor (must be opaque)
		"editor.wordHighlightStrongBackground": "#ffffff00", // Strong word match highlight for write-access symbols (must be opaque)
		"editor.wordHighlightTextBackground": "#ffffff00", // Background for text-based word highlighting (must be opaque)
		"editor.rangeHighlightBackground": "#ffffff00", // Background for range selection transparency effects (must be opaque)
		"editor.hoverHighlightBackground": "#ffffff00", // Background color shown when hovering over symbols (must be opaque)
		"editor.findMatchBackground": "#ffffff", // Background color for the current search result highlight
		"editor.findMatchHighlightBackground": "#ffffff00", // Background for other search matches in the document (must be opaque)
		"editor.findRangeHighlightBackground": "#ffffff00", // Background for the search range selection area (must be opaque)
		"editor.foldBackground": "#ffffff00", // Background color for folded/collapsed code sections (must be opaque)
		"editorCursor.foreground": "#ffffff", // Color of the text cursor/caret in the editor
		"editorLink.activeForeground": "#ffffff", // Color of clickable links when activated or clicked
		"editorWhitespace.foreground": "#ffffff", // Color of whitespace characters (spaces, tabs) when visible
		"editorIndentGuide.background1": "#ffffff", // Color of indentation guide lines showing code structure
		"editorIndentGuide.activeBackground1": "#ffffff", // Color of the active indentation guide line
		"editorRuler.foreground": "#ffffff", // Color of vertical ruler lines at specified column positions
		"editorBracketMatch.background": "#ffffff", // Background color for matching bracket pairs
		"editorBracketMatch.border": "#ffffff", // Border color around matching bracket pairs
		"editorBracketHighlight.foreground1": "#ffffff", // Color for first level of bracket pair highlighting
		"editorBracketHighlight.foreground2": "#ffffff", // Color for second level of bracket pair highlighting
		"editorBracketHighlight.foreground3": "#ffffff", // Color for third level of bracket pair highlighting
		"editorOverviewRuler.border": "#ffffff", // Border color of the overview ruler on the right side of editor
		// --- Editor Widgets & Popups ---
		"editorWidget.background": "#ffffff", // Background of floating widgets like parameter hints and quick info
		"editorWidget.border": "#ffffff", // Border color around editor widgets and floating panels
		"editorSuggestWidget.background": "#ffffff", // Background of autocomplete suggestion dropdown
		"editorSuggestWidget.border": "#ffffff", // Border around the suggestion widget dropdown
		"editorSuggestWidget.foreground": "#ffffff", // Text color for items in suggestion dropdown
		"editorSuggestWidget.highlightForeground": "#ffffff", // Color for highlighted matching text in suggestions
		"editorSuggestWidget.selectedBackground": "#ffffff", // Background of currently selected suggestion item
		"editorHoverWidget.background": "#ffffff", // Background of hover tooltip that shows on symbol hover
		"editorHoverWidget.border": "#ffffff", // Border around hover tooltip widgets
		"editorGhostText.foreground": "#ffffff", // Color for ghost text suggestions like Copilot inline completions
		"editorHint.foreground": "#ffffff", // Color for hint decorations in editor (info squiggles)
		"editorInfo.foreground": "#ffffff", // Color for informational squiggly underlines
		"editorWarning.foreground": "#ffffff", // Color for warning squiggly underlines
		"editorError.foreground": "#ffffff", // Color for error squiggly underlines and problem indicators
		// --- Editor Gutter ---
		"editorGutter.background": "#ffffff", // Background of the line number area and gutter margin
		"editorGutter.addedBackground": "#ffffff", // Background color for added lines in git diff gutter
		"editorGutter.modifiedBackground": "#ffffff", // Background color for modified lines in git diff gutter
		"editorGutter.deletedBackground": "#ffffff", // Background color indicating deleted lines in git diff
		"editorGutter.foldingControlForeground": "#ffffff", // Color of expand/collapse arrows for code folding
		"editorLineNumber.foreground": "#ffffff", // Color of line numbers in the gutter
		"editorLineNumber.activeForeground": "#ffffff", // Color of line number for the currently active line
		// --- Editor Inlay Hints ---
		"editorInlayHint.background": "#ffffff", // Background color for inlay hints like parameter names and type annotations
		"editorInlayHint.foreground": "#ffffff", // Text color for general inlay hint content
		"editorInlayHint.typeBackground": "#ffffff", // Background specifically for type inlay hints
		"editorInlayHint.typeForeground": "#ffffff", // Text color specifically for type inlay hints
		"editorInlayHint.parameterBackground": "#ffffff", // Background for parameter name inlay hints
		"editorInlayHint.parameterForeground": "#ffffff", // Text color for parameter name inlay hints
		// --- Editor Groups & Tabs ---
		"editorGroup.border": "#ffffff", // Border between split editor groups and panels
		"editorGroup.dropBackground": "#ffffff", // Background when dragging files over editor group for drop operation
		"editorGroupHeader.tabsBackground": "#ffffff", // Background of the tab bar area above editors
		"editorGroupHeader.noTabsBackground": "#ffffff", // Background of editor group header when tabs are disabled
		"tab.activeBackground": "#ffffff", // Background color of the currently active editor tab
		"tab.activeForeground": "#ffffff", // Text color of the currently active editor tab
		"tab.activeModifiedBorder": "#ffffff", // Border color indicating unsaved changes in active tab
		"tab.inactiveBackground": "#ffffff", // Background color of inactive editor tabs
		"tab.inactiveForeground": "#ffffff", // Text color of inactive editor tabs
		"tab.inactiveModifiedBorder": "#ffffff", // Border indicating unsaved changes in inactive tabs
		"tab.border": "#ffffff", // Border color around individual tabs
		"tab.hoverBackground": "#ffffff", // Background color when hovering over tabs
		"tab.unfocusedActiveModifiedBorder": "#ffffff", // Modified indicator for active tab when editor group is unfocused
		"tab.unfocusedHoverBackground": "#ffffff", // Hover background for tabs in unfocused editor groups
		"tab.unfocusedInactiveModifiedBorder": "#ffffff", // Modified indicator for inactive tabs in unfocused groups
		"tab.lastPinnedBorder": "#ffffff", // Border separating pinned tabs from regular tabs
		// --- Activity Bar ---
		"activityBar.background": "#ffffff", // Background of the vertical icon bar on the far left
		"activityBar.foreground": "#ffffff", // Color of icons in the activity bar
		"activityBar.inactiveForeground": "#ffffff", // Color of inactive icons in activity bar
		"activityBar.activeBorder": "#ffffff", // Border color around the currently active activity bar item
		"activityBar.border": "#ffffff", // Right border of the activity bar separating it from sidebar
		"activityBarBadge.background": "#ffffff", // Background of notification badges on activity bar icons
		"activityBarBadge.foreground": "#ffffff", // Text color of notification badges on activity bar icons
		"activityBar.dropBorder": "#ffffff", // Border shown when dragging items over activity bar
		"activityErrorBadge.background": "#ffffff", // Background for error notification badges
		"activityErrorBadge.foreground": "#ffffff", // Text color for error notification badges
		"activityWarningBadge.background": "#ffffff", // Background for warning notification badges
		"activityWarningBadge.foreground": "#ffffff", // Text color for warning notification badges
		// --- Side Bar ---
		"sideBar.background": "#ffffff", // Background of the sidebar containing file explorer and other views
		"sideBar.foreground": "#ffffff", // Default text color for sidebar content
		"sideBar.border": "#ffffff", // Border separating sidebar from editor area
		"sideBarTitle.foreground": "#ffffff", // Text color of sidebar section titles like "EXPLORER"
		"sideBarSectionHeader.background": "#ffffff", // Background of section headers in sidebar
		"sideBarSectionHeader.foreground": "#ffffff", // Text color of section headers in sidebar
		"sideBarSectionHeader.border": "#ffffff", // Border color of section headers in sidebar
		"sideBar.dropBackground": "#ffffff", // Background shown when dropping files into sidebar
		// --- Status Bar ---
		"statusBar.background": "#ffffff", // Background of the status bar at the bottom of the window
		"statusBar.foreground": "#ffffff", // Text color of status bar items
		"statusBar.border": "#ffffff", // Top border of the status bar
		"statusBar.debuggingBackground": "#ffffff", // Status bar background when debugging
		"statusBar.debuggingForeground": "#ffffff", // Status bar text color when debugging
		"statusBar.noFolderBackground": "#ffffff", // Status bar background when no folder is open
		"statusBar.noFolderForeground": "#ffffff", // Status bar text color when no folder is open
		"statusBarItem.activeBackground": "#ffffff", // Background of clicked status bar items
		"statusBarItem.hoverBackground": "#ffffff", // Background when hovering over status bar items
		"statusBarItem.remoteBackground": "#ffffff", // Background for remote status indicators
		"statusBarItem.remoteForeground": "#ffffff", // Text color for remote status indicators
		"statusBarItem.errorBackground": "#ffffff", // Background for error status items
		"statusBarItem.errorForeground": "#ffffff", // Text color for error status items
		// --- Title Bar ---
		"titleBar.activeBackground": "#ffffff", // Background of the title bar when window is focused
		"titleBar.activeForeground": "#ffffff", // Text color of title bar when window is focused
		"titleBar.inactiveBackground": "#ffffff", // Background of title bar when window is unfocused
		"titleBar.inactiveForeground": "#ffffff", // Text color of title bar when window is unfocused
		"titleBar.border": "#ffffff", // Border color separating title bar from content
		// --- Panel ---
		"panel.background": "#ffffff", // Background of bottom panel containing terminal and output
		"panel.border": "#ffffff", // Border color around the bottom panel
		"panel.dropBorder": "#ffffff", // Border shown when dropping items over panel
		"panelTitle.activeBorder": "#ffffff", // Bottom border of active panel tab
		"panelTitle.activeForeground": "#ffffff", // Text color of active panel tab
		"panelTitle.inactiveForeground": "#ffffff", // Text color of inactive panel tabs
		"panelInput.border": "#ffffff", // Border color of input fields in panels
		// --- Terminal ---
		"terminal.background": "#ffffff", // Background of terminal panels
		"terminal.foreground": "#ffffff", // Default text color in terminal
		"terminal.border": "#ffffff", // Border color around terminal panels
		"terminalCursor.background": "#ffffff", // Background color of terminal cursor
		"terminalCursor.foreground": "#ffffff", // Color of terminal cursor when it's a block
		"terminal.ansiBlack": "#ffffff", // Terminal ANSI black color for command output
		"terminal.ansiBlue": "#ffffff", // Terminal ANSI blue color for command output
		"terminal.ansiCyan": "#ffffff", // Terminal ANSI cyan color for command output
		"terminal.ansiGreen": "#ffffff", // Terminal ANSI green color for command output
		"terminal.ansiMagenta": "#ffffff", // Terminal ANSI magenta color for command output
		"terminal.ansiRed": "#ffffff", // Terminal ANSI red color for command output
		"terminal.ansiWhite": "#ffffff", // Terminal ANSI white color for command output
		"terminal.ansiYellow": "#ffffff", // Terminal ANSI yellow color for command output
		"terminal.ansiBrightBlack": "#ffffff", // Terminal bright black color for command output
		"terminal.ansiBrightBlue": "#ffffff", // Terminal bright blue color for command output
		"terminal.ansiBrightCyan": "#ffffff", // Terminal bright cyan color for command output
		"terminal.ansiBrightGreen": "#ffffff", // Terminal bright green color for command output
		"terminal.ansiBrightMagenta": "#ffffff", // Terminal bright magenta color for command output
		"terminal.ansiBrightRed": "#ffffff", // Terminal bright red color for command output
		"terminal.ansiBrightWhite": "#ffffff", // Terminal bright white color for command output
		"terminal.ansiBrightYellow": "#ffffff", // Terminal bright yellow color for command output
		"terminal.selectionBackground": "#ffffff", // Background color of selected text in terminal
		// --- Lists & Trees ---
		"list.activeSelectionBackground": "#ffffff", // Background of selected items in active lists
		"list.activeSelectionForeground": "#ffffff", // Text color of selected items in active lists
		"list.inactiveSelectionBackground": "#ffffff", // Background of selected items in unfocused lists
		"list.inactiveSelectionForeground": "#ffffff", // Text color of selected items in unfocused lists
		"list.hoverBackground": "#ffffff", // Background when hovering over list items
		"list.hoverForeground": "#ffffff", // Text color when hovering over list items
		"list.focusOutline": "#ffffff", // Outline color of focused list items
		"list.inactiveFocusOutline": "#ffffff", // Outline color of focused items in unfocused lists
		"list.errorForeground": "#ffffff", // Text color for error items in lists
		"list.warningForeground": "#ffffff", // Text color for warning items in lists
		"list.filterMatchBackground": "#ffffff", // Background of text that matches search filter
		"list.highlightForeground": "#ffffff", // Color of highlighted text in lists
		// --- Input Controls ---
		"input.background": "#ffffff", // Background of text input fields
		"input.foreground": "#ffffff", // Text color in input fields
		"input.border": "#ffffff", // Border color of input fields
		"input.placeholderForeground": "#ffffff", // Color of placeholder text in input fields
		"inputOption.activeBackground": "#ffffff", // Background of active input options like checkboxes
		"inputOption.activeBorder": "#ffffff", // Border of active input options
		"inputOption.activeForeground": "#ffffff", // Text color of active input options
		"inputOption.hoverBackground": "#ffffff", // Background when hovering over input options
		// --- Buttons & Badges ---
		"button.background": "#ffffff", // Background color of primary action buttons
		"button.foreground": "#ffffff", // Text color of primary action buttons
		"button.hoverBackground": "#ffffff", // Background when hovering over buttons
		"button.secondaryBackground": "#ffffff", // Background of secondary action buttons
		"button.secondaryForeground": "#ffffff", // Text color of secondary action buttons
		"button.secondaryHoverBackground": "#ffffff", // Background when hovering over secondary buttons
		"badge.background": "#ffffff", // Background of notification badges and counters
		"badge.foreground": "#ffffff", // Text color of notification badges and counters
		// --- Dropdown ---
		"dropdown.background": "#ffffff", // Background of dropdown selection menus
		"dropdown.foreground": "#ffffff", // Text color in dropdown menus
		"dropdown.border": "#ffffff", // Border color around dropdown controls
		"dropdown.listBackground": "#ffffff", // Background of dropdown option lists
		// --- Peek View ---
		"peekView.border": "#ffffff", // Border around peek definition and reference panels
		"peekViewEditor.background": "#ffffff", // Background of the editor area in peek view
		"peekViewEditor.matchHighlightBackground": "#ffffff", // Highlight color for matches in peek editor
		"peekViewResult.background": "#ffffff", // Background of the results list in peek view
		"peekViewResult.fileForeground": "#ffffff", // Color of file names in peek results
		"peekViewResult.lineForeground": "#ffffff", // Color of line numbers in peek results
		"peekViewResult.matchHighlightBackground": "#ffffff", // Highlight for matching text in peek results
		"peekViewResult.selectionBackground": "#ffffff", // Background of selected item in peek results
		"peekViewResult.selectionForeground": "#ffffff", // Text color of selected item in peek results
		"peekViewTitle.background": "#ffffff", // Background of peek view title bar
		"peekViewTitleDescription.foreground": "#ffffff", // Color of description text in peek title
		"peekViewTitleLabel.foreground": "#ffffff", // Color of main title text in peek view
		// --- Merge Conflicts ---
		"merge.border": "#ffffff", // Border color around merge conflict sections
		"merge.commonContentBackground": "#ffffff00", // Background for common content in merge conflicts (must be opaque)
		"merge.commonHeaderBackground": "#ffffff00", // Header background for common content sections (must be opaque)
		"merge.currentContentBackground": "#ffffff00", // Background for current branch content (must be opaque)
		"merge.currentHeaderBackground": "#ffffff00", // Header background for current branch (must be opaque)
		"merge.incomingContentBackground": "#ffffff00", // Background for incoming changes (must be opaque)
		"merge.incomingHeaderBackground": "#ffffff00", // Header background for incoming changes (must be opaque)
		// --- Notifications & Settings ---
		"notifications.background": "#ffffff", // Background of notification popup panels
		"notifications.border": "#ffffff", // Border color around notification panels
		"notifications.foreground": "#ffffff", // Text color in notification messages
		"notificationLink.foreground": "#ffffff", // Color of clickable links in notifications
		"notificationsErrorIcon.foreground": "#ffffff", // Color of error icons in notifications
		"notificationsInfoIcon.foreground": "#ffffff", // Color of info icons in notifications
		"notificationsWarningIcon.foreground": "#ffffff", // Color of warning icons in notifications
		"settings.headerForeground": "#ffffff", // Color of section headers in settings panel
		"settings.textInputForeground": "#ffffff", // Text color in settings input fields
		"settings.modifiedItemIndicator": "#ffffff", // Indicator color for modified settings
		// --- Git Decorations ---
		"gitDecoration.addedResourceForeground": "#ffffff",
		"gitDecoration.modifiedResourceForeground": "#ffffff",
		"gitDecoration.deletedResourceForeground": "#ffffff",
		"gitDecoration.untrackedResourceForeground": "#ffffff",
		"gitDecoration.ignoredResourceForeground": "#ffffff",
		"gitDecoration.conflictingResourceForeground": "#ffffff",
		"gitDecoration.submoduleResourceForeground": "#ffffff",
		// --- Text Links ---
		"textLink.foreground": "#ffffff", // Color of clickable links in text content
		"textLink.activeForeground": "#ffffff", // Color of links when clicked or hovered
		"descriptionForeground": "#ffffff", // Color of description text in various UI elements
		// --- Debug ---
		"debugToolBar.background": "#ffffff", // Background of the debug toolbar
		"debugIcon.breakpointForeground": "#ffffff", // Color of breakpoint icons in debug
		"debugIcon.startForeground": "#ffffff", // Color of debug start button icon
		"debugIcon.pauseForeground": "#ffffff", // Color of debug pause button icon
		"debugIcon.stopForeground": "#ffffff", // Color of debug stop button icon
		"debugIcon.disconnectForeground": "#ffffff", // Color of debug disconnect button icon
		"debugIcon.restartForeground": "#ffffff", // Color of debug restart button icon
		"debugIcon.stepOverForeground": "#ffffff", // Color of step over button icon
		"debugIcon.stepIntoForeground": "#ffffff", // Color of step into button icon
		"debugIcon.stepOutForeground": "#ffffff", // Color of step out button icon
		"debugIcon.continueForeground": "#ffffff", // Color of continue execution button icon
		// --- Charts ---
		"charts.foreground": "#ffffff", // Default text color in chart elements
		"charts.lines": "#ffffff", // Color of chart grid lines and axes
		"charts.red": "#ffffff", // Red color variant for chart data series
		"charts.blue": "#ffffff", // Blue color variant for chart data series
		"charts.yellow": "#ffffff", // Yellow color variant for chart data series
		"charts.orange": "#ffffff", // Orange color variant for chart data series
		"charts.green": "#ffffff", // Green color variant for chart data series
		"charts.purple": "#ffffff", // Purple color variant for chart data series
		// --- Extensions & Welcome Page ---
		"extensionButton.prominentBackground": "#ffffff", // Background of prominent extension action buttons
		"extensionButton.prominentForeground": "#ffffff", // Text color of prominent extension buttons
		"extensionButton.prominentHoverBackground": "#ffffff", // Background of extension buttons on hover
		"extensionBadge.remoteBackground": "#ffffff", // Background of remote extension badges
		"extensionBadge.remoteForeground": "#ffffff", // Text color of remote extension badges
		"welcomePage.progress.background": "#ffffff", // Background of progress bars on welcome page
		"welcomePage.progress.foreground": "#ffffff", // Foreground of progress indicators on welcome page
		"welcomePage.tileBackground": "#ffffff", // Background of welcome page tiles/cards
		"welcomePage.tileBorder": "#ffffff", // Border color around welcome page tiles
		"welcomePage.tileHoverBackground": "#ffffff" // Background of welcome page tiles on hover
